    "bcrypt": "^4.0.1",
    "bootstrap": "^4.4.1",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.3.16",
    "passport": "^0.4.1",
